{
  "name": "wedding-vip",
  "version": "1.0.1",
  "private": true,
  "description": "Wedding Website",
  "scripts": {
    "dev": "concurrently \"npm:dev:*\"",
    "dev:pages": "wrangler pages dev ./public --compatibility-date=2025-10-03",
    "dev:vite": "vite build --watch",
    "dev:css": "tailwindcss -i ./src/styles/globals.css -o ./public/build/enhance.css --watch",
    "build": "npm run build:css && npm run build:js",
    "build:css": "tailwindcss -i ./src/styles/globals.css -o ./public/build/enhance.css --minify",
    "build:js": "vite build",
    "migrate": "wrangler d1 migrations apply TODO_your_d1_database_name",
    "migrate:local": "wrangler d1 migrations apply TODO_your_d1_database_name --local",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:crypto": "vitest run tests/crypto",
    "test:security": "vitest run tests/security",
    "test:concurrency": "vitest run tests/concurrency",
    "test:meta": "vitest run tests/meta",
    "test:types": "tsc --noEmit --skipLibCheck",
    "test:duplication": "jscpd src functions --min-lines 5 --min-tokens 50 --threshold 5",
    "test:dead-code": "ts-prune --project tsconfig.json",
    "test:all": "npm run test:types && npm run test && npm run test:duplication && npm run test:dead-code",
    "typecheck": "tsc --noEmit",
    "prebuild": "rm -rf public/build",
    "predeploy": "npm run build",
    "deploy": "wrangler pages deploy ./public"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250913.0",
    "@tailwindcss/forms": "^0.5.9",
    "@types/node": "^22.5.5",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "glob": "^11.0.0",
    "jscpd": "^4.0.5",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "ts-prune": "^0.10.3",
    "typescript": "^5.9.2",
    "vite": "^7.1.5",
    "vitest": "^3.2.4",
    "wrangler": "^4.40.3",
    "zod": "^3.23.8"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.544.0",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-day-picker": "^9.10.0",
    "react-dom": "^19.1.1",
    "sonner": "^2.0.7",
    "stripe": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7"
  }
}
